// dot -Tps out.dot -o out.ps

graph "Abstract Syntax Tree"
{
	fontname="Helvetica,Arial,sans-serif"
	node [fontsize=10, width=".2", height=".2", margin=0]
	edge [fontsize=6]
	graph[fontsize=8];

	label="Abstract Syntax Tree"

	n0 ;
	n0 [label="modifier"] ;
	n0 -- n1 ;

	n1 ;
	n1 [label="public"] ;

}
